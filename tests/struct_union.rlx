#Include Memory.rlx
#Include String.rlx
#Include Console.rlx

struct Test {
	union {
		i8 B,
		i16 S,
		i32 L,
		i64 LL
	}
}


define i32 Main(i64 ArgC, i8** ArgV) {
	GetArgs(&ArgC, &ArgV)
	
	Test* T := Alloc(#Test)
	
	T->LL := AToI(ArgV[1])
	
	IWrite(T->B)
	WriteCharacter(' ')
	IWrite(T->S)
	WriteCharacter(' ')
	IWrite(T->L)
	WriteCharacter(' ')
	IWrite(T->LL)
	
	return 0
}
