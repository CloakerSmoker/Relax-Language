#Include ./src/lib/Memory.rlx
#Include ./src/lib/String.rlx
#Include ./src/lib/Console.rlx

struct Test {
	i64 Value,
	define i64 GetValue() {
		return this->Value
	},
	define void SetValue(i64 New) {
		this->Value := New
	}
}

define i32 Main() {
	
	Test* T := Alloc(#Test)
	
	IWrite(T->GetValue())
	
	T->SetValue(20059)
	
	WriteCharacter(',')
	
	IWrite(T->GetValue())
	
	WriteCharacter(' ')
	
	Test L
	
	L.SetValue(0)
	
	IWrite(L.GetValue())
	
	WriteCharacter(',')
	
	L.SetValue(20060)
	
	IWrite(L.GetValue())
	
	return 0
}
