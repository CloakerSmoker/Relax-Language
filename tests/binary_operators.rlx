#Include ./src/lib/Memory.rlx
#Include ./src/lib/String.rlx
#Include ./src/lib/Console.rlx

struct DumbTest {
	i8 IdiotField,
	void Dummy
}

define i32 Main() {
	IWrite(20 + 10)
	WriteCharacter(',')
	IWrite(20 - 10)
	WriteCharacter(',')
	IWrite(20 * 10)
	WriteCharacter(',')
	IWrite(20 / 10)
	WriteCharacter(',')
	IWrite(22 % 10)
	WriteCharacter(',')
	IWrite(20 & 30)
	WriteCharacter(',')
	IWrite(20 | 10)
	WriteCharacter(',')
	IWrite(20 ^ 10)
	WriteCharacter(',')
	IWrite(0 && 0)
	IWrite(0 && 1)
	IWrite(1 && 0)
	IWrite(1 && 1)
	WriteCharacter(',')
	IWrite(0 || 0)
	IWrite(0 || 1)
	IWrite(1 || 0)
	IWrite(1 || 1)
	WriteCharacter(',')
	IWrite(1 << 20)
	WriteCharacter(',')
	IWrite(1 << 20 >> 20)
	WriteCharacter(',')
	
	/* Todo: Write unary_operators.rlx and move these into that file */
	IWrite(~0)
	WriteCharacter(',')
	IWrite(~90 + 1)
	
	DumbTest D
	DumbTest* DP := &D
	
	WriteCharacter(',')
	IWrite(DP->IdiotField || false)
	IWrite(DP->IdiotField && true)
	
	WriteCharacter(',')
	IWrite(#i8)
	WriteCharacter(',')
	IWrite(#i16)
	WriteCharacter(',')
	IWrite(#i32)
	WriteCharacter(',')
	IWrite(#i64)
	WriteCharacter(',')
	IWrite(#i8*)
	WriteCharacter(',')
	IWrite(#DumbTest)
	
	
	WriteCharacter(' ')
	
	i32 Com := 0
	
	Com += 5
	Com -= 3
	
	IWrite(Com) /* 2 */
	WriteCharacter(',')
	
	Com *= 10 	/* 20 */
	Com /= 5	/* 4 */
	
	IWrite(Com)
	WriteCharacter(',')
	
	Com %= 3	/* 1 */
	
	IWrite(Com)
	WriteCharacter(',')
	
	Com |= 0xFF02	/* 0xFF03 */
	Com &= 0xFF		/* 3 */
	IWrite(Com)
	WriteCharacter(',')
	
	Com ^= 1	/* 2 */
	IWrite(Com)
	
	/* Assignment operators result in: 2, 4, 1, 3, 2 */
	
	return 1 >> 127
}
