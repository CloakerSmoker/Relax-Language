#Include Memory.rlx
#Include String.rlx
#Include Console.rlx

struct DumbTest {
	i8 IdiotField,
	void Dummy
}

define i32 Main() {
	IWrite(20 + 10)
	WriteCharacter(',')
	IWrite(20 - 10)
	WriteCharacter(',')
	IWrite(20 * 10)
	WriteCharacter(',')
	IWrite(20 / 10)
	WriteCharacter(',')
	IWrite(22 % 10)
	WriteCharacter(',')
	IWrite(20 & 30)
	WriteCharacter(',')
	IWrite(20 | 10)
	WriteCharacter(',')
	IWrite(20 ^ 10)
	WriteCharacter(',')
	IWrite(0 && 0)
	IWrite(0 && 1)
	IWrite(1 && 0)
	IWrite(1 && 1)
	WriteCharacter(',')
	IWrite(0 || 0)
	IWrite(0 || 1)
	IWrite(1 || 0)
	IWrite(1 || 1)
	WriteCharacter(',')
	IWrite(1 << 20)
	WriteCharacter(',')
	IWrite(1 << 20 >> 20)
	WriteCharacter(',')
	
	/* Todo: Write unary_operators.rlx and move these into that file */
	IWrite(~0)
	WriteCharacter(',')
	IWrite(~90 + 1)
	
	DumbTest D
	DumbTest* DP := &D
	
	WriteCharacter(',')
	IWrite(DP->IdiotField || false)
	IWrite(DP->IdiotField && true)
	
	return 0
}
