#Include ./src/lib/Memory.rlx
#Include ./src/lib/String.rlx
#Include ./src/lib/Console.rlx

struct inner_inner {
	i64 iia,
	i64 iib
}
struct inner {
	i64 ia,
	inner_inner ii,
	i64 ib
}
struct outer {
	i64 oa,
	
	union {
		inner oi,
		i64 ou
	},
	
	i64 ob
}

define i64 Main(i64 ArgC, i8** ArgV) {
	GetArgs(&ArgC, &ArgV)
	
	outer test
	
	outer* testp := &test
	
	test.oa := 10
	test.oi.ia := 20
	test.oi.ii.iia := 30
	test.oi.ii.iib := 40
	test.oi.ib := 50
	test.ob := 60
	
	Print(testp->oa)
	PrintCharacter(' ')
	Print(test.oi.ia)
	PrintCharacter(' ')
	Print(testp->oi.ii.iia)
	PrintCharacter(' ')
	Print(test.oi.ii.iib)
	PrintCharacter(' ')
	Print(testp->oi.ib)
	PrintCharacter(' ')
	Print(test.ob)
	PrintCharacter(' ')
	
	test.ou := 70
	
	Print(test.oi.ia)
	
	return 0
}
