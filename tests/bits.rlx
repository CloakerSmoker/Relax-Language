#Require ./src/lib/Console.rlx

bits Example As i32 {
	[0:10] test,
	[20:25] test2,
	[0:31] test3
}

bits Example2 As i64 {
	[0:31] low_32,
	[32:63] high_32
}
bits Example3 As i64 {
	[0:47] a_pointer,
	[48:63] a_tag
}

define i32 Main() {
	Example E := 0 As Example
	
	E.test := 90
	E.test2 := 20
	E.test3 := 0
	
	Print(E As i64)
	PrintCharacter(',')
	
	Example2 E2 := 0x4000000090009000 As Example2
	
	PrintHex(E2.low_32 & 0xFFFFFFFF)
	PrintCharacter(',')
	PrintHex(E2.high_32)
	
	Example3 E3 := 0 As Example3
	i64 Test := 52893
	
	E3.a_pointer := &Test As i64
	E3.a_tag := 30
	
	PrintCharacter(',')
	Print(E3.a_pointer = &Test)
	
	Test := 9000
	
	Print(*(E3.a_pointer As i64*) = Test)
	
	return E As i32
}
