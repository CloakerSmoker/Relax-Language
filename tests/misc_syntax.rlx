#Include ./src/lib/Memory.rlx
#Include ./src/lib/String.rlx
#Include ./src/lib/Console.rlx

define void VATest(i64 T, ... Args) {
	IWriteHex(T)
	WriteLiteral(', ')
	IWriteHex(Args[0])
	WriteLiteral(' ')
	
	i32 C := Args[0] As i32
	Args += 8
	
	for (i32 I := 0, I < C, I += 1) {
		IWriteHex(Args[I])
		
		if (C - 1 != I) {
			WriteLiteral(', ')
		}
	}
}


define i32 Main() {
	IWriteHex('aaaaaaaa')
	WriteLiteral(', ')
	IWriteHex('aaaaaaa')
	WriteLiteral(', ')
	IWriteHex('aaaaa')
	WriteLiteral(', ')
	IWriteHex('aaaa')
	WriteLiteral(', ')
	IWriteHex('aaa')
	WriteLiteral(', ')
	IWriteHex('aa')
	WriteLiteral(', ')
	IWriteHex('a')
	
	WriteLiteral(' ')
	
	VATest(10, 1, 2, 3)
	
	return 0
}
